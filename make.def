# cross compiler tools defined
CROSS_COMPILE ?=

AS      = $(CROSS_COMPILE)as
LD      = $(CROSS_COMPILE)ld
CC      = $(CROSS_COMPILE)gcc -g
CPP     = $(CC) -E
AR      = $(CROSS_COMPILE)ar
NM      = $(CROSS_COMPILE)nm
STRIP   = $(CROSS_COMPILE)strip
RANLIB  = $(CROSS_COMPILE)ranlib

#local host tools defined
CP      := cp
RM      := rm
MKDIR   := mkdir
SED     := sed
FIND    := find
MKDIR   := mkdir
XARGS   := xargs

